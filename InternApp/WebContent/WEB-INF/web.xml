<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
	<display-name>InternApp</display-name>

	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>TableServlet</servlet-name>
		<servlet-class>net.jincorporation.servlet.TableServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>TableServlet</servlet-name>
		<url-pattern>/TableServlet</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>ResultServlet</servlet-name>
		<servlet-class>net.jincorporation.servlet.ResultServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>ResultServlet</servlet-name>
		<url-pattern>/ResultServlet</url-pattern>
	</servlet-mapping>

</web-app>